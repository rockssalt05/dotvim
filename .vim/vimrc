call plug#begin()
	Plug 'luochen1990/rainbow'
	Plug 'neoclide/coc.nvim', {'branch': 'release'}
	"to install coc.clangd: 'CocInstall coc-clangd'
	"to install clangd: 'CocCommand clangd.install'
call plug#end()

set linebreak
set breakindent

set tabstop=4
set shiftwidth=0

set number
set termguicolors
colorscheme codedark

let g:rainbow_active = 0
